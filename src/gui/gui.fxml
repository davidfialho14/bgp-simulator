<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="pane" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.65"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" maxWidth="65.0" minWidth="65.0" prefWidth="65.0"/>
        <ColumnConstraints hgrow="NEVER" maxWidth="85.0" minWidth="85.0" prefWidth="85.0"/>
        <ColumnConstraints hgrow="NEVER" maxWidth="90.0" minWidth="90.0" prefWidth="90.0"/>
        <ColumnConstraints hgrow="NEVER" maxWidth="90.0" minWidth="90.0" prefWidth="90.0"/>
        <ColumnConstraints hgrow="NEVER" maxWidth="90.0" minWidth="90.0" prefWidth="90.0"/>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="70.0" minWidth="70.0" prefWidth="70.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="NEVER"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="NEVER"/>
        <RowConstraints maxHeight="250.0" minHeight="-Infinity" prefHeight="226.0" vgrow="NEVER"/>
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="-Infinity" prefHeight="5.0" vgrow="ALWAYS"/>
    </rowConstraints>
    <children>
        <Label fx:id="networkLabel" text="Network:"/>
        <TextField fx:id="networkTextField" GridPane.columnIndex="1" GridPane.columnSpan="5"/>
        <Button fx:id="browseButton" mnemonicParsing="false" onAction="#handleClickedBrowseButton" text="Browse"
                GridPane.columnIndex="6"/>
        <Label text="Destination:" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label text="Repetitions:" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
        <Spinner fx:id="destinationIdSpinner" editable="true" prefHeight="26.0" prefWidth="111.0"
                 GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Spinner fx:id="repetitionsSpinner" editable="true" GridPane.columnIndex="4" GridPane.rowIndex="1"/>
        <Button fx:id="startButton" defaultButton="true" disable="true" maxWidth="67.0" minWidth="67.0"
                mnemonicParsing="false" onAction="#handleClickedStartButton" prefWidth="67.0" text="Start"
                GridPane.columnIndex="6" GridPane.rowIndex="1"/>
        <TitledPane animated="false" prefHeight="301.0" prefWidth="385.0" text="Partial Deployment"
                    GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="2">
            <content>
                <fx:include fx:id="partialDeploymentForm" source="partialdeployment/partial_deployment.fxml"/>
            </content>
        </TitledPane>
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
    </padding>
</GridPane>
